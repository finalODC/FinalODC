		<mapper resource="mappers/hospitalsearch-mapper.xml"/>
		<mapper resource="mappers/managerAsk-mapper.xml"/>
		<mapper resource="mappers/notice-mapper.xml"/>
	</mappers>
	
	
	
</configuration>
