		<typeAlias type="com.ohdogcat.odc.qna.model.vo.Qna" alias="Qna"/>
		<typeAlias alias="QnaReply" type="com.ohdogcat.odc.qna.model.vo.QnaReply"/>
		<typeAlias type="com.ohdogcat.odc.manager.Notice.model.vo.Notice" alias="Notice"/>
		
